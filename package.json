{
  "name": "creaptive",
  "version": "1.0.0",
  "description": "A bridge for the gap between the skilled and the creative in Africa and people that need their output",
  "main": "creaptive.js",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development babel-watch server/app.js",
    "start:prod": "npm run build && npm run serve",
    "serve": "cross-env NODE_ENV=production node buildServer/app.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "rimraf client/ui && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/app.js -o buildServer/app.js",
    "clean": "rimraf client/ui buildServer",
    "precommit": "lint-staged --verbose",
    "cm": "git-cz",
    "lint": "eslint --fix src server webpack",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint"
  },
  "keywords": [
    "Skill",
    "Creative",
    "Freelance",
    "Africa",
    "Design",
    "Software",
    "Active",
    "Apt",
    "Craft"
  ],
  "author": "Henry Otuadinma | creaptive.com",
